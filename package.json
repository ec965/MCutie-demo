{
  "name": "mcutie",
  "version": "0.1.0",
  "description": "mqtt backend & frontend",
  "main": "index.js",
  "scripts": {
    "server": "npm start --prefix backend",
    "server-prod": "npm run prod --prefix backend",
    "client": "npm start --prefix backend/client",
    "client-prod": "npm run prod --prefix backend/client",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "start": "npm run server",
    "init": "npm install --prefix client && npm install --prefix backend && npm install",
    "heroku-postbuild": "npm run init && npm run build --prefix backend/client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ec965/MCutie.git"
  },
  "author": "Enoch Chau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ec965/MCutie/issues"
  },
  "homepage": "https://github.com/ec965/MCutie#readme",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  },
  "dependencies": {}
}
